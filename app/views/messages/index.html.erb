<div id='content'>
  <h1><%= "#{@dog.name}'s Messages" %></h1>
  <a href="/messages/new"><button type='button' id="send_message_button" class="button-blue">Send a Message</button></a>
  <div id='inbox'>
  Inbox
    <%= render 'partials/received_messages' %>
  </div>
  <div id='outbox'>
  Sent
    <%= render 'partials/sent_messages' %>
  </div>
</div>
